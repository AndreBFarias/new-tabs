{
  "manifest_version": 2,
  "name": "Dracula new tab",
  "version": "1.2",
  "description": "New page in dracula style!",
  "content_scripts": [
    {
      "match_about_blank": true,
      "matches": ["about:blank"],
      "js": ["script.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
